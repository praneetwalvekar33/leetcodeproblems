package math;
//	Factors of a number(Brute Force)
public class FactorsOfNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 30;
		factor(num);
	}
	private static void factor(int num) {
		for(int i = 1; i <= num; i++) {
			if(num % i == 0) {
				System.out.print(i + " ");
			}
		}
	}
	

}
